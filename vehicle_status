from driver_profile import vehicle_type
Vehicle_Type_Selected = vehicle_type()
vehicle_status = input("Enter your vehicle status (Good/Bad) ")
if vehicle_status.lower() == "good":
    print("No need to send for maintenance!")
else:
    print("Send for maintenance!")
    from define_maintenance_form import maintenance_form
    Car_Plate, UserID, Service_Type, Repair_Cost, Maintenance_Date = maintenance_form()